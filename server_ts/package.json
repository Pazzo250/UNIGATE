{
  "name": "unigat-server-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "*",
    "google-auth-library": "*",
    "bcrypt": "*",
    "cookie-parser": "*",
    "cors": "*",
    "dotenv": "*",
    "express": "*",
    "jsonwebtoken": "*"
  },
  "devDependencies": {
    "prisma": "*",
    "ts-node": "*",
    "ts-node-dev": "*",
    "typescript": "*",
    "@types/bcrypt": "*",
    "@types/google-auth-library": "*",
    "@types/express": "*",
    "@types/node": "*",
    "@types/jsonwebtoken": "*",
    "@types/cookie-parser": "*",
    "@types/bcrypt": "*"
  }
}
